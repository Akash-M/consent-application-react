{
  "name": "ui-consents",
  "version": "0.0.1",
  "description": "Consent UI app based on react",
  "keywords": [
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Akash-M/consent-application-react.git",
    "directory": "packages/frontend/ui-consents"
  },
  "author": "Akash-M <akash.manjunath@gmail.com>",
  "engines": {
    "node": ">=v14.17.6"
