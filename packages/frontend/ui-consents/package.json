  "scripts": {
    "build": "CI=false webpack --config=webpack.production.config.ts --mode=production",
    "lint:prettier": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
    "lint:scss": "stylelint src/**/*.scss",
    "lint:ts": "run exec:eslint --cache-file $(pwd)/.eslintcache $(pwd)",
    "lint": "yarn lint:ts && yarn prettier && yarn lint:scss",
    "prettier": "prettier -c \"src/**/*.{js,ts,tsx}\"",
    "start:dev": "webpack serve --config webpack.development.config.ts --mode=development",
    "start:docker": "run exec:docker-compose up -d --build ui-consents && run exec:docker-compose logs -f ui-consents",
    "test:unit": "jest --config test/unit/jest.config.js --coverage",
    "test:unit:update": "yarn test:unit -u",
    "test:unit:watch": "yarn test:unit --watch"
  },
