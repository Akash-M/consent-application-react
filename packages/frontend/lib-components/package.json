    "prettier": "prettier -c \"src/**/*.{js,ts,tsx,scss}\"",
    "prettier:fix": "prettier --write \"**/*.{js,ts,tsx,scss}\"",
    "start:dev": "start-storybook -p 6000",
    "test:unit": "jest --config test/unit/jest.config.js --coverage --passWithNoTests",
    "test:unit:update": "yarn test:unit -u",
    "test:unit:watch": "yarn test:unit --watch",
    "test": "yarn test:unit"
  },
