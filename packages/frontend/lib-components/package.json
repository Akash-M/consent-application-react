    "lint:prettier": "prettier --write \"./**/*.{js,jsx,json}\"",
    "lint:scss": "stylelint src/**/*.scss",
    "lint:ts": "run exec:eslint --cache-file $(pwd)/.eslintcache $(pwd)",
